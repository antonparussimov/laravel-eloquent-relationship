{
  "name": "ankurk91/laravel-eloquent-relationships",
  "description": "Add some more eloquent relationships to Laravel php framework.",
  "keywords": [
    "laravel",
    "eloquent",
    "BelongsToOne",
    "MorphToOne"
  ],
  "homepage": "https://github.com/ankurk91/laravel-eloquent-relationships",
  "license": "MIT",
  "authors": [
    {
      "name": "ankurk91",
      "homepage": "https://ankurk91.github.io"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "illuminate/database": "5.7.*|5.8.*",
    "illuminate/support": "5.7.*|5.8.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload": {
    "psr-4": {
      "Ankurk91\\Eloquent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
