{
  "name": "ankurk91/laravel-eloquent-relationships",
  "description": "Add missing eloquent relationships to Laravel php framework.",
  "keywords": [
    "laravel",
    "eloquent",
    "BelongsToOne",
    "MorphToOne"
  ],
  "homepage": "https://github.com/ankurk91/laravel-eloquent-relationships",
  "license": "MIT",
  "authors": [
    {
      "name": "ankurk91",
      "homepage": "https://ankurk91.github.io"
    }
  ],
  "require": {
    "php": "^7.2||^8.0",
    "illuminate/database": "^6.0||^7.0||^8.0",
    "illuminate/support": "^6.0||^7.0||^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3.4||^9.0.1"
  },
  "autoload": {
    "psr-4": {
      "Ankurk91\\Eloquent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "scripts": {
    "test": "vendor/bin/phpunit --testdox --color=always"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
